bool isPalindrome(int x) {
        
        if(x < 0)
            return false;
        long long b = 0, a = 0 , c=x;
        while(c != 0)
        {
            a = c%10;
            c/=10;
            b = b*10 + a;
}
        
        if(b == x)
            return true;
        else
            return false;
        
    }
